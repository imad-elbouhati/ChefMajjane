<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/home_nav_graph"
    app:startDestination="@id/changePasswordFragment">

    <fragment
        android:id="@+id/homeFragment"
        android:name="com.majjane.chefmajjane.AccueilFragment"
        android:label="fragment_home"
        tools:layout="@layout/fragment_accueil" >
        <action
            android:id="@+id/action_accueilFragment_to_espaceSushiFragment"
            app:destination="@id/espaceSushiFragment" />
    </fragment>
    <fragment
        android:id="@+id/espaceSushiFragment"
        android:name="com.majjane.chefmajjane.views.FoodListFragment"
        android:label="fragment_espace_sushi"
        tools:layout="@layout/fragment_espace_sushi" >
        <action
            android:id="@+id/action_espaceSushiFragment_to_sushiDetailsFragment"
            app:destination="@id/sushiDetailsFragment" />
        <argument
            android:name="category_bundle"
            app:argType="com.majjane.chefmajjane.responses.AccueilResponseItem"
            />
        <action
            android:id="@+id/action_espaceSushiFragment_to_mesCommandesFragment"
            app:destination="@id/mesCommandesFragment" />

    </fragment>
    <fragment
        android:id="@+id/sushiDetailsFragment"
        android:name="com.majjane.chefmajjane.FoodDetailsFragment"
        android:label="fragment_sushi_details"
        tools:layout="@layout/fragment_sushi_details" >
        <argument
            android:name="article_bundle"
            app:argType="com.majjane.chefmajjane.responses.Article" />
        <action
            android:id="@+id/action_sushiDetailsFragment_to_espaceSushiFragment"
            app:destination="@id/espaceSushiFragment" />
    </fragment>
    <fragment
        android:id="@+id/mesCommandesFragment"
        android:name="com.majjane.chefmajjane.views.MesCommandesFragment"
        android:label="fragment_mes_commandes"
        tools:layout="@layout/fragment_mes_commandes" >
        <argument
            android:name="article_list_bundle"
            app:argType="com.majjane.chefmajjane.responses.Article[]" />
        <action
            android:id="@+id/action_mesCommandesFragment_to_mapsFragment"
            app:destination="@id/mapsFragment" />
        <action
            android:id="@+id/action_mesCommandesFragment_to_displayCommandsFragment"
            app:destination="@id/displayCommandsFragment" />
    </fragment>
    <fragment
        android:id="@+id/languageFragment"
        android:name="com.majjane.chefmajjane.views.LanguageFragment"
        android:label="fragment_language"
        tools:layout="@layout/fragment_language" />
    <fragment
        android:id="@+id/contactFragment"
        android:name="com.majjane.chefmajjane.views.ContactFragment"
        android:label="fragment_contact"
        tools:layout="@layout/fragment_contact" />
    <fragment
        android:id="@+id/myProfileFragment"
        android:name="com.majjane.chefmajjane.views.MyProfileFragment"
        android:label="fragment_my_profile"
        tools:layout="@layout/fragment_my_profile" >
        <action
            android:id="@+id/action_myProfileFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/changePasswordFragment"
        android:name="com.majjane.chefmajjane.ChangePasswordFragment"
        android:label="fragment_change_password"
        tools:layout="@layout/fragment_change_password" >
        <action
            android:id="@+id/action_changePasswordFragment_to_homeFragment"
            app:destination="@id/homeFragment" />
    </fragment>
    <fragment
        android:id="@+id/mapsFragment"
        android:name="com.majjane.chefmajjane.views.MapsFragment"
        android:label="fragment_maps"
        tools:layout="@layout/fragment_maps" >
        <action
            android:id="@+id/action_mapsFragment_to_mesCommandesFragment"
            app:destination="@id/mesCommandesFragment" />
    </fragment>
    <fragment
        android:id="@+id/displayCommandsFragment"
        android:name="com.majjane.chefmajjane.views.DisplayCommandsFragment"
        android:label="fragment_display_commands"
        tools:layout="@layout/fragment_display_commands" />
    <fragment
        android:id="@+id/reclamationFragment"
        android:name="com.majjane.chefmajjane.views.ReclamationFragment"
        android:label="fragment_reclamation"
        tools:layout="@layout/fragment_reclamation" >
        <action
            android:id="@+id/action_reclamationFragment_to_homeFragment"
            app:destination="@id/homeFragment"
            />
    </fragment>
    <fragment
        android:id="@+id/signUpFragment"
        android:name="com.majjane.chefmajjane.views.auth.SignUpFragment"
        android:label="fragment_sign_up"
        tools:layout="@layout/fragment_sign_up" />
</navigation>